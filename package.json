{
  "name": "lz-string",
  "author": {
    "name": "pieroxy",
    "email": "pieroxy@pieroxy.net"
  },
  "contributors": [
    {
      "name": "Karnthis",
      "email": "me@erinrivas.com"
    },
    {
      "name": "Ryc O'Chet",
      "email": "rycochet@rycochet.com"
    }
  ],
  "version": "2.0.0-rc.1",
  "license": "MIT",
  "description": "LZ-based compression algorithm",
  "homepage": "http://pieroxy.net/blog/pages/lz-string/index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/pieroxy/lz-string.git"
  },
  "bugs": {
    "url": "https://github.com/pieroxy/lz-string/issues"
  },
  "keywords": [
    "lz",
    "compression",
    "string"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "format": "prettier --write src",
    "start": "vite",
    "test": "vitest src.main.test.ts",
    "test:all": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src"
  },
  "dependencies": {
    "tiny-invariant": "1.3.1",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@btmills/prettier": "3.1.0",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/node": "20.11.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@vitest/coverage-v8": "1.2.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "rollup-plugin-typescript-paths": "1.5.0",
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vite-plugin-dts": "3.7.0",
    "vite-plugin-eslint": "1.8.1",
    "vite-tsconfig-paths": "4.2.3",
    "vitest": "1.2.0"
  },
  "override": {
    "prettier": "npm:@btmills/prettier@3.1.0"
  }
}
